{
"manifest_version": 3,
"name": "CODERROR",
"version": "0.42.3",
"description": "Игра CODERROR в виде расширения для Chrome",
"icons": {
	"16": "images/icon.png",
	"48": "images/icon.png", 
	"128": "images/icon.png"
},
"action":{
	"default_title":"Запустить CODERROR"
},
"background":{
	"service_worker":"background.js"
},
"permissions": [
	"storage",
	"unlimitedStorage"
],
"content_security_policy": {
	"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
},
"sandbox": {
	"pages": ["CODERROR.html"]
},
"web_accessible_resources": [{
	"resources": ["game/*", "images/*", "scripts/*"],
	"matches": ["<all_urls>"]
}]
}
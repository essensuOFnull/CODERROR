{
"manifest_version":3,
"name":"CODERROR",
"version":"0.38.1",
"description":"Игра CODERROR в виде расширения для Chrome",
"icons":{
	"16":"images/icon.png",
	"48":"images/icon.png", 
	"128":"images/icon.png"
},
"action":{
	"default_title":"Запустить CODERROR"
},
"background":{
	"service_worker":"background.js"
},
"permissions":[
	"storage",
	"unlimitedStorage"
],
"content_security_policy":{
	"extension_pages":"script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
},
"web_accessible_resources":[{
	"resources":["*"],
	"matches":["<all_urls>"]
}]
}